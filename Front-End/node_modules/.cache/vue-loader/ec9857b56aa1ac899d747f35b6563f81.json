on>\n      </b-form>\n    </b-card>\n  <b-alert show variant=\"secondary\" class=\"mt-3\">\n  <strong>Credenciais de demonstração:</strong><br />\n  Email: <code>pedro.pereira@uab.pt</code><br />\n  Password: <code>PWA2026!</code>\n</b-alert>\n\n        </b-col>\n    </b-row>\n  </b-container>\n</template>\n\n<script>\nexport default {\n  data() {\n    return { email: '', password: '', loading: false };\n  },\n methods: {\n  async submit() {\n    console.log('SUBMIT chamado', this.email);\n    this.loading = true;\n    try {\n      const res = await this.$store.dispatch('auth/login', {\n        email: this.email,\n        password: this.password\n      });\n      console.log('LOGIN OK', res);\n      this.$router.push('/minhas-propostas');\n    } catch (e) {\n      console.error('ERRO NO LOGIN', e);\n      const msg =\n        e.response && e.response.data && e.response.data.message\n          ? e.response.data.message\n          : 'Erro no login.';\n      alert(msg);\n    } finally {\n      this.loading = false;\n    }\n  }\n}\n\n};\n</script>\n"]}]}